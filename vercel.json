{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"  // This is the output folder Vite uses
        }
      }
    ],
    "routes": [
      {
        "src": "/.*",
        "dest": "/index.html"
      }
    ]
  }
  